{
  "name": "learn-express",
  "version": "1.0.0",
  "main": "main.ts",
  "repository": "git@github.com:gn-t-k/learn-express.git",
  "author": "gn-t-k <gntk1995422@gmail.com>",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write --loglevel=warn 'src/**/*.{js,ts,gql,graphql,json}'",
    "lint-conflict": "eslint-config-prettier 'src/**/*.{js,ts}'",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "check-all": "yarn type-check && yarn lint && yarn format && yarn test",
    "start": "node ./dist/main.js",
    "start-ts": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "build": "tsc",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "postinstall": "typesync"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/prettier": "^2.1.6",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "^2.20.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.1.3",
    "typesync": "^0.8.0"
  },
  "dependencies": {
    "@prisma/client": "2.20.1",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "jest-mock-extended": "^1.0.15",
    "uuid": "^8.3.2"
  }
}
